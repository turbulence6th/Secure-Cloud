{
  "manifest_version": 2,
  "name": "Secure Cloud",
  "version": "1.0",
  "minimum_chrome_version": "23",
  "icons": {
    "16": "icon_16.png",
    "128": "icon_128.png"
  },
  "app": {
    "background": {
      "scripts": [
                "config.js",
                "jquery.js",
                "main.js",
                "generate_key.js",
                "owncloud_login.js",
                "encrypt.js",
                "download.js",
                "share.js"

      ]
    }
  },
"externally_connectable": {
  "matches": ["*://10.100.0.93/*", "*://localhost/*"]
}
,"key": "endtoend",
 "permissions": [
  "storage"
 ]

}
