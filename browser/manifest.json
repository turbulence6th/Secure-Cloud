{
    "manifest_version": 2,
    
    "name": "Secure Cloud",
    "description": "A Cloud Storage System with End to End Security",
    "version": "1.0",
    
    "browser_action": {
        "default_icon": "images/icon.png",
        "default_popup": "popup.html",
        "default_title": "Secure Cloud!"
    },
    "content_scripts": [{
                        "matches": ["<all_urls>"],
                        "js": [
                                "jquery.js",
                               "encrypt.js"
                               ],
                        "run_at": "document_end"
                        }
                        ],
    "permissions": [
					"storage",
                    "activeTab",
					"tabs","<all_urls>",
                    "https://ajax.googleapis.com/"
                    ]
       
}
